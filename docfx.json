{
  "build": {
    "content": [
      {
        "files": [
          "*.md",
          "*.js"
        ],
        "exclude": [
          "docs/**",
          "plugin/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/*.jpg",
          "images/btn-*.png"
        ],
        "exclude": [
          "docs/**",
          "plugin/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadata": {
      "_appTitle": "willnode's Projects",
      "_disableNavbar": true,
      "_disableBreadcrumb": true,
      "_disableContribution": true,
      "_appFooter": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}